{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python@4.1.0",
            "config": {
                "maxLambdaSize": "50mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}